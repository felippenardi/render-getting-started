{
  "pages": {
    "getting-started": {
      "path": "/getting-started",
      "theme": "theme",
      "extensible": true
    },
    "getting-started/nested-a": {
      "path": "/getting-started/nested-a"
    },
    "getting-started/nested-b": {
      "path": "/getting-started/nested-b"
    },
    "getting-started/nested-param": {
      "path": "/getting-started/:slug/nested-param"
    },
    "getting-started/nested-param/deeply-nested": {
      "path": "/getting-started/:slug/nested-param/:deep/deeply-nested"
    },
    "getting-started/container": {
      "path": "/getting-started/container"
    },
    "legacy-getting-started": {
      "path": "/legacy-extensions/render-example",
      "theme": "theme"
    },
    "legacy-without-tachyons": {
      "path": "/legacy-extensions/render-example-without-tachyons",
      "theme": "theme",
      "disableExternals": ["tachyons"]
    },
    "dynamic": {
      "path": "/dynamic"
    }
  },
  "extensions": {
    "getting-started": {
      "component": "index",
      "props": {
        "to": "/getting-started/nested-a"
      }
    },
    "getting-started/nested-a": {
      "component": "nested",
      "props": {
        "to": "/getting-started/nested-b"
      }
    },
    "getting-started/nested-b": {
      "component": "nested",
      "props": {
        "to": "/getting-started/myparam/nested-param"
      }
    },
    "getting-started/nested-param": {
      "component": "nested",
      "props": {
        "to": "/getting-started/myparam/nested-param/second/deeply-nested"
      }
    },
    "getting-started/nested-param/deeply-nested": {
      "component": "nested",
      "props": {
        "to": "/getting-started/container"
      }
    },
    "getting-started/container": {
      "component": "vtex.render-runtime/ExtensionContainer"
    },
    "getting-started/container/first": {
      "component": "nested",
      "props": {
        "to": "/getting-started"
      }
    },
    "getting-started/container/second": {
      "component": "nested",
      "props": {
        "to": "/getting-started/nested-a"
      }
    },
    "getting-started/first-step": {
      "component": "first"
    },
    "getting-started/second-step": {
      "component": "second",
      "async": true
    },
    "getting-started/configurable-component": {
      "component": "configurable",
      "props": {
        "show": true,
        "title": "This is a configurable component",
        "text": "You can edit it yourself in the edit bar below."
      }
    },
    "legacy-getting-started": {
      "component": "index"
    },
    "legacy-getting-started/first-step": {
      "component": "first"
    },
    "legacy-getting-started/second-step": {
      "component": "second"
    },
    "legacy-without-tachyons/first-step": {
      "component": "first"
    },
    "legacy-without-tachyons/second-step": {
      "component": "second"
    },
    "dynamic": {
      "component": "dynamic"
    },
    "dynamic/banner": {
      "component": "section",
      "props": {
        "title": "Banner",
        "text": "A banner would fit nicely here!"
      }
    },
    "dynamic/description": {
      "component": "section",
      "props": {
        "title": "Description",
        "text": "This refrigerator is so nice! All your foodstuffs fit into it!"
      }
    },
    "dynamic/benefits": {
      "component": "section",
      "props": {
        "title": "Benefits",
        "text": "Buying this refrigerator will definitely make your life better. By an observable amount."
      }
    },
    "dynamic/price": {
      "component": "section",
      "props": {
        "title": "Price",
        "text": "All this refrigerator goodness could be yours for the low low price of $1000"
      }
    }
  }
}
